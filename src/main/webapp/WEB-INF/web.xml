<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	
	<context-param>
	    <param-name>VAT_RATE</param-name>
	    <param-value>12</param-value>
  	</context-param>
  
	<!-- SERVLET ROUTES -->
	<servlet>
		<servlet-name>App</servlet-name>
		<display-name>App</display-name>
		<description></description>
		<servlet-class>controller.App</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Process</servlet-name>
		<display-name>Process</display-name>
		<description></description>
		<servlet-class>controller.Process</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Remove</servlet-name>
		<display-name>Remove</display-name>
		<description></description>
		<servlet-class>controller.Remove</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Add</servlet-name>
		<display-name>Add</display-name>
		<description></description>
		<servlet-class>controller.Add</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Item</servlet-name>
		<display-name>Item</display-name>
		<description></description>
		<servlet-class>controller.Item</servlet-class>
	</servlet>
	
	
	
	
	
	
	<!-- SERVLET MAPPINGS -->
	<servlet-mapping>
		<servlet-name>App</servlet-name>
		<url-pattern>/App</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Process</servlet-name>
		<url-pattern>/Process</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Remove</servlet-name>
		<url-pattern>/Remove</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Add</servlet-name>
		<url-pattern>/Add</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Item</servlet-name>
		<url-pattern>/Item</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
    	<welcome-file>App</welcome-file>
	</welcome-file-list>
</web-app>
